[user]
    name = <YOUR NAME>
    email = <YOUR@EMAIL.COM>
[github]
    user = <YOUR GITHUB USERNAME>
[alias]
    co = checkout
    com = checkout master
    mm = merge master
    pushup = push --set-upstream
    gignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
[color]
  	branch = auto
  	diff = auto
  	status = auto
[color "branch"]
  	current = yellow reverse
  	local = yellow
  	remote = green
[color "diff"]
  	meta = yellow bold
  	frag = magenta bold
  	old = red bold
  	new = green bold
[color "status"]
  	added = yellow
  	changed = green
  	untracked = cyan
